					<!-- Datos -->
					<vbox align="end" pack="center">
						<label id="txtStudent"
							style="color:white; font-size:12px;background: #532821; border:none" />
						<label id="txtClave"
							style="color:white; font-size:8px;background: #532821;border:none" />
					
					</vbox>

					<!-- Cerrar Sesión -->
					<vbox width="50px" align="end">

					</vbox>

				</hbox>
			</hbox>
		</div>
	</window>



</zk>
